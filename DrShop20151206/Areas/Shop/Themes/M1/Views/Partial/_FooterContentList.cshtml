@model   List<Maticsoft.Model.CMS.Content>
<dl class="help-center-about" itemid="helpcenterabout" itemname="@ViewBag.contentclassName">
                <dt>@ViewBag.contentclassName</dt>
  
        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
                  <dd>
                  <a href="/Article/Details/@item.ContentID" target="_blank">@(Maticsoft.Common.StringPlus.SubString(item.Title, 8, ""))  </a>
                   </dd>
            }
        }
        
        
   
 </dl>
